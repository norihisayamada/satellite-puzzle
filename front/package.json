{
  "name": "satellite-puzzle",
  "version": "1.0.0",
  "description": "satellite-puzzle project",
  "author": "K.odeveloper10@gmail.com",
  "private": true,
  "engines": {
    "node": "10",
    "npm": "6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.vue": [
      "stylelint --fix",
      "eslint --fix",
      "git add"
    ]
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue src/**/*.vue",
    "lint:js:fix": "eslint --fix .js,.vue src/**/*.vue",
    "lint:css": "stylelint 'src/**/*.vue'",
    "lint:css:fix": "stylelint --fix src/**/*.vue",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "@nuxtjs/dotenv": "^1.3.0",
    "@nuxtjs/pwa": "^2.6.0",
    "@nuxtjs/style-resources": "^0.1.1",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "forever": "^0.15.3",
    "husky": "^1.2.1",
    "initd-forever": "^0.1.8",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.11.0",
    "nuxt": "^2.0.0",
    "nuxt-property-decorator": "^1.2.0",
    "nuxt-sass-resources-loader": "^2.0.5",
    "path": "^0.12.7",
    "prettier": "^1.15.3",
    "pug": "2.0.3",
    "pug-plain-loader": "^1.0.0",
    "ress": "^1.2.2",
    "sprintf-js": "^1.1.2",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0",
    "vuex-class": "^0.3.1",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^4.19.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^4.0.0",
    "sass-loader": "^7.1.0",
    "ts-loader": "^3.5.0"
  }
}
